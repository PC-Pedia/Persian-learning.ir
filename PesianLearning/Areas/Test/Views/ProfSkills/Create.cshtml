@model Models.ProfSkill

@{
    ViewData["Title"] = "Create";
}



<h4>ProfSkill</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProfID" class="control-label"></label>
                <select asp-for="ProfID" class ="form-control" asp-items="ViewBag.ProfID"></select>
            </div>
            <div class="form-group">
                <label asp-for="SkillID" class="control-label"></label>
                <select asp-for="SkillID" class ="form-control" asp-items="ViewBag.SkillID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="ثبت" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-danger" asp-action="Index">بازگشت</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
